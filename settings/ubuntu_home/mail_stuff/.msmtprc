defaults
tls on
tls_starttls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account default
host smtp.gmail.com
port 587
auth on
user USER_NAME@gmail.com
# passwordeval cmd_for_password_evaluation
# or save password of the user in gnome-keyring in advance
from USER_NAME@gmail.com
# logfile ~/msmtp.log
